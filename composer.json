{
    "name": "bnussbau/trmnl-pipeline-cmd",
    "description": "",
    "keywords": ["trmnl", "laravel zero", "console", "cli"],
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "bnussbau/trmnl-pipeline-php": "^0.3.1",
        "laravel-zero/framework": "^12.0.2"
    },
    "require-dev": {
        "laravel/pint": "^1.25.1",
        "mockery/mockery": "^1.6.12",
        "pestphp/pest": "^3.8.4|^4.1.2",
        "phpacker/phpacker": "^0.6.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": ["trmnl-pipeline"]
}
